<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Register</servlet-name>
  	<display-name>Register</display-name>
  	<description></description>
  	<servlet-class>system.onlinebanking.controller.Register</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>JAX-RS Servlet</servlet-name>
  	<description>JAX-RS Tools Generated - Do not modify</description>
  	<servlet-class></servlet-class>
  	<load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
  	<servlet-name>ClientServlet</servlet-name>
  	<display-name>ClientServlet</display-name>
  	<description></description>
  	<servlet-class>system.onlinebanking.controller.ClientServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>system.onlinebanking.controller.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Register</servlet-name>
  	<url-pattern>/Register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>JAX-RS Servlet</servlet-name>
  	<url-pattern>/jaxrs/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ClientServlet</servlet-name>
  	<url-pattern>/ClientServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
</web-app>
